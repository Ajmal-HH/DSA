/**
 * @param {number[]} nums
 * @return {number}
 */
var differenceOfSum = function(nums) {
    const sum = nums.reduce((a,b)=>a+b,0)
    let sum1 = 0
    for (let i = 0; i<nums.length;i++){
        let string = nums[i].toString().split('')
        for(let j=0;j<string.length;j++){
            sum1+=parseInt(string[j])
        }
    }
        return sum - sum1
};
