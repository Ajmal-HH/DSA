/**
 * @param {number[]} candies
 * @param {number} extraCandies
 * @return {boolean[]}
 */
var kidsWithCandies = function(candies, extraCandies) {
    const max = Math.max(...candies)
    let arr = []
    for(let i=0;i<candies.length;i++){
        let result = candies[i] + extraCandies
        if(result>=max){
            arr.push(true)
        }else{
            arr.push(false)
        }
    }
    return arr
};
